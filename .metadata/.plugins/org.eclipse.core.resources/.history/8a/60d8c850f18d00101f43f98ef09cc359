package ch1;

public class Basic2 {
// CTRL + / 한 줄 주석
/*
* 문서의 정보, 소개
*/
/*
 여러 줄 주석 
 변수란?
 데이터를 저장하는 그릇 
 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		int : 정수
		int age = 20;
		System.out.println(30);
		System.out.println(age);
		System.out.println("age");
		
		System.out.println("=====");
//		double : 실수
		double weight = -42.123;
		System.out.println(weight);
//		String : 문자열
//		boolean : 참/거짓 ( true, false )
	}

}
