package ch1;

import java.util.Scanner;

public class Control2 {
	public static void main(String[] args) {
		// Scanner 
		
		// 90점 이상이면 A, 80점 이상이면 B , 70점 이상이면 C , 나머지 D 
		
		Scanner scanner = new Scanner(System.in);
		int score = 0;
		String result = "A";
		if(score >= 90) {
			result = "A";
		}else if(score >= 80) {
			result = "B";
		}else if(score >= 70) {
			result = "C";
		}else {
			result = "D";
		}
		System.out.println("결과는 : "+ result + "입니다.");
	}
}
